<div>

  <% unless user_me?(user) %>
    <div id="follow_form">
    <% if current_user.following?(@user) %>
      <%= render 'users/unfollow' %>
    <% elsif current_user.requested?(@user) %>
      <%= render 'users/requested'%>
    <%else%>
      <%= render 'users/follow' %>
    <% end %>
    </div>
  <% end %>
</div>
